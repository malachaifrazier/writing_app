  <%= month_calendar attribute: :deadline, events: projects, previous_link: events_ajax_previous_link, next_link: events_ajax_next_link do |date, projects| %>

    <%= date.strftime("%e") %>
     <div class="calendar-entries tooltip">
        <% projects.each do |project| %>
          <div class="calendar-entry">
            <%= link_to "", project_path(project) %>
          </div>
        <% end %>
      </div>
  <% end %>
